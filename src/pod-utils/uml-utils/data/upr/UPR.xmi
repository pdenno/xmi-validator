      <packagedElement xmi:id="UPR." xmi:type="uml:Association" name="">
        <memberEnd xmi:idref="UPR.ComparisonOperator.semantics"/>
        <memberEnd xmi:idref="UPR.OperatorSemantics.notation"/>
      </packagedElement>
      <packagedElement xmi:id="UPR.Operators.Library" xmi:type="uml:Package" URI="" name="Library">
        <packagedElement xmi:id="UPR.Operators.Library.neq_operator" xmi:type="uml:LiteralString" name="neq_operator" value="!=">
          <ownedComment xmi:id="UPR.Operators.Library.neq_operator._comment0" xmi:type="uml:Comment" body="Defines a &amp;quot;not equal to&amp;quot; operator"/>
        </packagedElement>
        <packagedElement xmi:id="UPR.NEQ_Operator" xmi:type="uml:Class" name="NEQ_Operator">
          <generalization xmi:id="UPR.NEQ_Operator._generalization.UPR.OperatorSemanticsClass" xmi:type="uml:Generalization">
            <general xmi:idref="UPR.OperatorSemanticsClass"/>
          </generalization>
          <ownedComment xmi:id="UPR.NEQ_Operator._comment0" xmi:type="uml:Comment" body="This class defines the semantics for a &amp;quot;not equal to&amp;quot; relationship"/>
          <ownedOperation xmi:id="UPR.NEQ_Operator.applyTo_Element_Element" xmi:type="uml:Operation" isQuery="true" name="applyTo">
            <bodyCondition xmi:id="UPR.NEQ_Operator.applyTo_Element_Element._rule.body_condition" xmi:type="uml:Constraint" name="body_condition">
              <specification 
              xmi:id="UPR.NEQ_Operator.applyTo_Element_Element._rule.body_condition.specification" xmi:type="uml:OpaqueExpression" body="if (x.oclIsKindOf(Real) and y.oclIsKindOf(Real)) then
	x.oclAsType(Real) &amp;lt;&amp;gt; y.oclAsType(Real)
else
	OclInvalid
endif" language="OCL" name="specification">
</specification>
            </bodyCondition>
            <ownedParameter xmi:id="UPR.NEQ_Operator.applyTo_Element_Element.result" xmi:type="uml:Parameter" direction="return" effect="create" name="result">
              <type href="http://www.omg.org/spec/UML/20131001/PrimitiveTypes.xmi#Boolean"/>
            </ownedParameter>
            <ownedParameter xmi:id="UPR.NEQ_Operator.applyTo_Element_Element.x" xmi:type="uml:Parameter" effect="read" name="x">
              <type href="http://www.omg.org/spec/UML/20131001/UML.xmi#Element"/>
            </ownedParameter>
            <ownedParameter xmi:id="UPR.NEQ_Operator.applyTo_Element_Element.y" xmi:type="uml:Parameter" effect="read" name="y">
              <type href="http://www.omg.org/spec/UML/20131001/UML.xmi#Element"/>
            </ownedParameter>
            <ownedRule xmi:idref="UPR.NEQ_Operator.applyTo_Element_Element._rule.body_condition"/>
          </ownedOperation>
        </packagedElement>
        <packagedElement xmi:id="UPR.GTE_Operator" xmi:type="uml:Class" name="GTE_Operator">
          <generalization xmi:id="UPR.GTE_Operator._generalization.UPR.OperatorSemanticsClass" xmi:type="uml:Generalization">
            <general xmi:idref="UPR.OperatorSemanticsClass"/>
          </generalization>
          <ownedComment xmi:id="UPR.GTE_Operator._comment0" xmi:type="uml:Comment" body="This class defines the semantics for a &amp;quot;greater than or equal to&amp;quot; relationship"/>
          <ownedOperation xmi:id="UPR.GTE_Operator.applyTo_Element_Element" xmi:type="uml:Operation" isQuery="true" name="applyTo">
            <bodyCondition xmi:id="UPR.GTE_Operator.applyTo_Element_Element._rule.body_condition" xmi:type="uml:Constraint" name="body_condition">
              <specification xmi:id="UPR.GTE_Operator.applyTo_Element_Element._rule.body_condition.specification" xmi:type="uml:OpaqueExpression" body="if (x.oclIsKindOf(Real) and y.oclIsKindOf(Real)) then x.oclAsType(Real) &amp;gt;= y.oclAsType(Real) else OclInvalid endif" language="OCL" name="specification"/>
            </bodyCondition>
            <ownedParameter xmi:id="UPR.GTE_Operator.applyTo_Element_Element.result" xmi:type="uml:Parameter" direction="return" effect="create" name="result">
