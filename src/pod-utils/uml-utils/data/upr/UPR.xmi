            <specification xmi:id="UPR.DesignConstraint.constrainedUseCase_._rule.body_condition.specification0" xmi:type="uml:OpaqueExpression" body="self.base_Constraint.constrainedElement-&amp;gt;at(1).oclAsType(UML::UseCase)" name=""/>
          </bodyCondition>
          <ownedParameter xmi:id="UPR.DesignConstraint.constrainedUseCase_.result" xmi:type="uml:Parameter" direction="return" effect="read" name="result">
            <type href="http://www.omg.org/spec/UML/20131001/UML.xmi#UseCase"/>
