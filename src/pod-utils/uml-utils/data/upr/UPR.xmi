        <ownedRule xmi:id="UPR.DCtoDCSensitivity._rule.client_and_supplier_designconstraints" xmi:type="uml:Constraint" name="client_and_supplier_designconstraints">
          <ownedComment xmi:id="UPR.DCtoDCSensitivity._rule.client_and_supplier_designconstraints._comment0" xmi:type="uml:Comment" body="Both the client and the supplier shall have the DesignConstraint stereotype applied"/>
          <specification 
          xmi:id="UPR.DCtoDCSensitivity._rule.client_and_supplier_designconstraints.specification" xmi:type="uml:OpaqueExpression" body="DesignConstraint.allInstances().base_Constraint-&amp;gt;includesAll(self.base_Dependency.client-&amp;gt;union(self.base_Dependency.supplier))
" language="OCL" name="specification">
            <type href="http://www.omg.org/spec/UML/20131001/PrimitiveTypes.xmi#Boolean"/>
</specification>
        </ownedRule>
